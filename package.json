{
  "name": "sammons-logger",
  "maintainers": [
    "sammons"
  ],
  "version": "0.0.9",
  "engines": {
    "node": "6.x.x"
  },
  "description": "typescript logger based on winston",
  "main": "built/index.js",
  "scripts": {
    "coverage": "nyc --all --reporter=lcov --exclude-after-remap=false npm test",
    "test": "mocha --require source-map-support/register ./built/test/**/*-test.js",
    "build": "tsc --build --watch ./test",
    "build-sync": "rm -rf built && tsc --build ./test",
    "prepublishOnly": "rm -rf ./built && tsc --build ./src"
  },
  "repository": {
    "type": "git",
    "url": "github.com/sammons/slogger"
  },
  "keywords": [
    "logging",
    "typescript",
    "node.js",
    "javascript",
    "microservices"
  ],
  "author": "sammons",
  "license": "MIT",
  "dependencies": {
    "source-map-support": "^0.5.9",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "@types/sinon": "^5.0.2",
    "chai": "^4.1.2",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "sinon": "^6.1.5",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.0.3"
  },
  "files": [
    "index.d.ts",
    "built"
  ]
}
